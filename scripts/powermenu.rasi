configuration {
    font: "JetBrainsMono Nerd Font 14";
}

window {
    background-color: #000000;
    border: 1px solid #1793d1;
    border-radius: 8px;
    width: 250px;
}

entry {
    background-color: #111111;
    color: #eeeeee;
    placeholder: "Power Menu";
}

listview {
    background-color: #000000;
    padding: 5px;
}

element {
    color: #eeeeee;
    padding: 8px;
}

element selected {
    background-color: #1793d1;
    color: #000000;
}
